{ 
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    //"extends": [ "eslint:recommended", "prettier"], 
    //"extends": [ "airbnb", "prettier"],
    // airbnb-base
    "extends": ["airbnb-base","plugin:prettier/recommended"],
    "plugins": ["prettier"], 
    "rules": {
        "prettier/prettier": "error",
        "no-console" : "off",
        "consistent-return" : "warn",
        "no-else-return" : "off"
    }
}